using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Lemonade.Frame.Tools;
using System.Windows.Forms;

namespace Lemonade.ToolBar.Datas
{
    /// <summary>
    /// 工具栏下拉框数据对象
    /// </summary>
    public class TComboBox  
    {
        /// <summary>
        /// 下拉框项类
        /// </summary>
        public class ToolsComboBoxItem
        {
            /// <summary>
            /// 标题
            /// </summary>
            public string Title { get; set; }
            /// <summary>
            /// 值
            /// </summary>
            public string Value { get; set; }

            public override string ToString()
            {
                return this.Title;
            }
        }
        /// <summary>
        /// 
        /// </summary>
        public string ToolsBarCode
        {
            get;
            set;
        }

        /// <summary>
        /// 分组名称
        /// </summary>
        public string GroupName
        {
            get;
            set;
        }
         
        /// <summary>
        /// 索引
        /// </summary>
        public int ItemIndex
        {
            get;
            set;
        }

        /// <summary>
        /// 下拉框中的项列表
        /// </summary>
        public List<ToolsComboBoxItem> Items { get; set; }
 
         
 
    }
}
